{
  "name": "n7tools",
  "version": "1.0.0",
  "scripts": {
    "serve": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start": "cd backend && node server.mjs",
    "start:frontend": "cd frontend && parcel index.html",
    "start:backend": "cd backend && node server.mjs",
    "build": "cd frontend && parcel build index.html --dist-dir dist --public-url /N7Tools",
    "build:css": "tailwindcss build -i ./frontend/output.css -o ./frontend/dist/output.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
